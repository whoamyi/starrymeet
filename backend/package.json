{
  "name": "starrymeet-backend",
  "version": "1.0.0",
  "description": "StarryMeet Backend API",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "backup:json": "ts-node scripts/export-data-json.ts",
    "restore:json": "ts-node scripts/restore-from-json.ts",
    "seed:backup": "ts-node src/scripts/real-celebrity-seeding/01-backup-and-prep.ts",
    "seed:celebrities": "ts-node src/scripts/real-celebrity-seeding/02-main-seeding.ts",
    "seed:reprice": "ts-node src/scripts/real-celebrity-seeding/03-reprice-celebrities.ts",
    "seed:fix-pricing": "ts-node src/scripts/real-celebrity-seeding/04-fix-pricing-with-real-data.ts",
    "seed:realistic-pricing": "ts-node src/scripts/real-celebrity-seeding/05-realistic-pricing-all-tiers.ts",
    "availability:generate": "ts-node src/scripts/availability/generate-availability.ts",
    "availability:cleanup": "ts-node src/services/availability/cleanup.service.ts",
    "migrate:create-schema": "ts-node -e \"require('./src/migrations/create-normalized-schema').up(require('./src/config/database').default.getQueryInterface())\"",
    "migrate:transfer-data": "ts-node src/scripts/migrate-to-normalized-schema.ts",
    "enrich:celebrities": "ts-node src/scripts/enrich-normalized-celebrities.ts",
    "classify:celebrities": "ts-node src/scripts/classification-agent.ts",
    "classify:new-only": "CLASSIFY_DATE_FILTER=2025-10-23 ts-node src/scripts/classification-agent.ts",
    "seed:young-emerging": "ts-node src/scripts/real-celebrity-seeding/06-young-emerging-celebrities.ts",
    "seed:young-insert": "ts-node src/scripts/real-celebrity-seeding/07-insert-and-classify-young.ts",
    "seed:massive-young": "ts-node src/scripts/real-celebrity-seeding/08-massive-young-celebrity-seeding.ts",
    "seed:enhanced-round2": "ts-node src/scripts/real-celebrity-seeding/09-enhanced-young-seeding.ts",
    "seed:insert-round2": "ts-node src/scripts/real-celebrity-seeding/10-insert-round2.ts"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "resend": "^6.2.1",
    "sequelize": "^6.35.1",
    "stripe": "^14.5.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
