    <script>
        function calculateEarnings() {
            const meetings = parseInt(document.getElementById('meetingsPerMonth').value) || 0;
            const price = parseInt(document.getElementById('pricePerMeeting').value) || 0;
            const monthly = meetings * price;
            const annual = monthly * 12;
            document.getElementById('monthlyEarnings').textContent = '$' + monthly.toLocaleString();
            document.getElementById('annualEarnings').textContent = '$' + annual.toLocaleString();
        }
        calculateEarnings();
    </script>
